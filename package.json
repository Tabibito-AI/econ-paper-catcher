{
  "name": "paper-read-web-app",
  "version": "1.0.0",
  "description": "A web application to read and filter academic papers.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node app.mjs",
    "build": "webpack --config webpack.config.cjs",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "ci": "npm install && git add package.json package-lock.json && git commit -m 'Update package.json and package-lock.json' || echo 'No changes to commit'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomoto0/paper-read-web-app.git"
  },
  "author": "Tomoto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomoto0/paper-read-web-app/issues"
  },
  "homepage": "https://tomoto0.github.io/paper-read-web-app/",
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "aws-sdk": "^2.814.0",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "gh-pages": "^3.2.3",
    "jsdom": "^25.0.1",
    "node-fetch": "^3.3.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "babel-loader": "^9.1.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  }
